		"@remix-run/react": "^2.6.0",
		"@remix-run/serve": "^2.6.0",
		"@supabase/supabase-js": "^2.39.3",
		"isbot": "^4.1.0",
		"jotai": "^2.6.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.5.3",
		"@remix-run/dev": "^2.6.0",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"daisyui": "latest",
		"eslint": "^8.38.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.28.1",
